<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.edu.unimelb.eldercare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="HomeScreen"
            android:label="@string/HomeText">
        </activity>

        <activity
            android:name="UserProfileUI"
            android:label="@string/name_placeholder">
	    </activity>
	
	    <activity
            android:name="SettingsUI"
            android:label="@string/SettingsHeading">
        </activity>

        <activity
            android:name="FrequentContactsUI"
            android:label="@string/frequentContactsHeading">
        </activity>

        <activity
            android:name="ChangeDNameActivity"
            android:label="@string/changeDNameHeading">
        </activity>

        <activity
            android:name="MessagingActivity"
            android:label="Messaging">
        </activity>
    </application>

</manifest>